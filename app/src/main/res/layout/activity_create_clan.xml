<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    android:id="@+id/createClan"
    android:background="@drawable/dojo2"
    android:weightSum="7"
    android:padding="0sp">

    <!-- Seção da esquerda para seleção de cores -->

    <LinearLayout
    android:id="@+id/layer0"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

        <Space
            android:layout_width="match_parent"
            android:layout_height="55sp"
            android:gravity="center"
            android:orientation="horizontal" />


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="100sp"
            android:gravity="center"
            android:layout_marginLeft="37sp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="200sp"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                app:srcCompat="@drawable/placa"/>

            <TextView
                    android:id="@+id/clanname"
                    android:layout_width="155sp"
                    android:layout_centerInParent="true"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20sp"
                    android:fontFamily="@font/samurai"
                    android:inputType="textCapWords"
                    android:textColor="@color/black"
                    android:text="CLAN"
                    android:textAlignment="center"
                    android:textSize="10sp" />

        </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="39sp"
        android:layout_marginTop="-10sp"
        android:layout_gravity="start"
        android:gravity="start">

        <ImageView
            android:id="@+id/vector_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:src="@drawable/bandeira"/>

        <ImageView
            android:id="@+id/clanicon"
            android:layout_width="50sp"
            android:layout_height="50sp"
            android:layout_gravity="center"
            android:layout_centerInParent="true"
            app:tint="@color/verde"
            android:src="@drawable/wave" />


        </RelativeLayout>





    </LinearLayout>


    <LinearLayout
        android:id="@+id/firstcolun"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:weightSum="7"
        android:padding="10sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:orientation="vertical"
            android:padding="0dp">


            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal">

                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="200sp"
                    android:layout_height="100sp"
                    android:layout_marginTop="-50sp"
                    android:layout_centerInParent="true"
                    app:srcCompat="@drawable/placa"/>

            <Button
                android:id="@+id/kendogiColor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/transparent"
                android:fontFamily="@font/samurai"
                android:text="Kendogi"
                android:textAlignment="center"
                android:layout_centerInParent="true"
                android:textColor="@color/black"
                android:textSize="16sp" />


            </RelativeLayout>


            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="-10sp"
                android:layout_gravity="center_horizontal">

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="200sp"
                android:layout_height="100sp"
                android:layout_marginTop="-50sp"
                android:layout_centerInParent="true"
                app:srcCompat="@drawable/placa"/>

            <Button
                android:id="@+id/hakamaColor"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/transparent"
                android:fontFamily="@font/samurai"
                android:text="Hakama"
                android:textAlignment="center"
                android:layout_centerInParent="true"
                android:textColor="@color/semi_transparent_black"
                android:textSize="16sp" />

            </RelativeLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="-10sp"
                android:layout_gravity="center_horizontal">

                <ImageView

                    android:layout_width="200sp"
                    android:layout_height="100sp"
                    android:layout_marginTop="-50sp"
                    android:layout_centerInParent="true"
                    app:srcCompat="@drawable/placa"/>

                <Button
                    android:id="@+id/simbolColor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:backgroundTint="@color/transparent"
                    android:fontFamily="@font/samurai"
                    android:text="Simbolo"
                    android:textAlignment="center"
                    android:layout_centerInParent="true"
                    android:textColor="@color/semi_transparent_black"
                    android:textSize="16sp" />

            </RelativeLayout>

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="500sp"
                android:layout_gravity="center_horizontal"
                android:paddingBottom="10sp">

                <!-- Primeira Linha -->
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/collorButtons"
                    android:gravity="center">

                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexmagenta"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:src="@drawable/hex_color2"
                            android:tag="collorButton"
                            app:tint="@color/magenta" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>
                </TableRow>

                <!-- Segunda Linha -->
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:translationY="-30dp">

                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexrosa"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/rosa" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>

                    <Space
                        android:layout_width="18dp"
                        android:layout_height="0dp" />

                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexroxo"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/roxo" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>
                </TableRow>

                <!-- Terceira Linha -->
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:translationY="-60dp">

                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexvermelho"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/vermelho" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>

                    <Space
                        android:layout_width="18dp"
                        android:layout_height="0dp" />

                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexpurpura"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/purpura" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>

                    <Space
                        android:layout_width="18dp"
                        android:layout_height="0dp" />

                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexmarinho"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/azul_marinho" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>
                </TableRow>

                <!-- Quarta Linha -->
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:translationY="-90dp">

                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexbordo"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:tag="collorButton"
                            android:focusable="true"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/bordô" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>

                    <Space
                        android:layout_width="18dp"
                        android:layout_height="0dp" />

                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexnaval"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:tag="collorButton"
                            android:focusable="true"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/azul_naval" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>
                </TableRow>

                <!-- Quinta Linha -->
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:translationY="-120dp">

                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexlaranja"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/laranja" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>

                    <Space
                        android:layout_width="18dp"
                        android:layout_height="0dp" />

                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexcinza"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/cinza" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>

                    <Space
                        android:layout_width="18dp"
                        android:layout_height="0dp" />

                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexceleste"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/azul_celeste" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>
                </TableRow>

                <!-- Sexta Linha -->
                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:translationY="-150dp">

                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexocre"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/ocre" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>

                    <Space
                        android:layout_width="18dp"
                        android:layout_height="0dp" />

                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexpetroleo"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/petroleo" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:translationY="-180dp">

                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexamarelo"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/amarelo" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>

                    <Space
                        android:layout_width="18dp"
                        android:layout_height="0dp"/>

                    <FrameLayout

                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexescuro"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/verde_escuro" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>

                    <Space
                        android:layout_width="18dp"
                        android:layout_height="0dp"/>

                    <FrameLayout

                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexciano"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/ciano" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>

                    <!-- Adicione outros hexágonos da linha aqui -->
                </TableRow>


                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:translationY="-210dp"> <!-- Move a segunda linha 25dp para cima -->

                    <FrameLayout

                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexlima"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/verde_lima" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>

                    <Space
                        android:layout_width="18dp"
                        android:layout_height="0dp"/>

                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexesmeralda"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/esmeralda" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>


                    <!-- Adicione outros hexágonos da linha aqui -->
                </TableRow>


                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:translationY="-240dp"> <!-- Move a segunda linha 25dp para cima -->

                    <FrameLayout

                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexverde"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/verde" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>


                    <!-- Adicione outros hexágonos da linha aqui -->
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:translationY="-270dp"> <!-- Move a segunda linha 25dp para cima -->

                    <FrameLayout

                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexpreto"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/black" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>

                    <Space
                        android:layout_width="8dp"
                        android:layout_height="0dp"></Space>


                    <FrameLayout
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:clickable="false"
                        android:focusable="false"></FrameLayout>

                    <Space
                        android:layout_width="8dp"
                        android:layout_height="0dp"></Space>


                    <FrameLayout

                        android:layout_width="50dp"
                        android:layout_height="50dp">

                        <ImageView
                            android:id="@+id/hexbranco"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clickable="true"
                            android:focusable="true"
                            android:tag="collorButton"
                            android:src="@drawable/hex_color2"
                            app:tint="@color/white" />

                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/hex2" />
                    </FrameLayout>

                </TableRow>


            </TableLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:gravity="center"
            android:orientation="vertical">

            <Space
                android:layout_width="wrap_content"

                android:layout_height="150sp"/>

            <ImageView
                android:id="@+id/samuraianimation"
                android:layout_width="200sp"
                android:layout_marginLeft="-20sp"
                android:layout_height="200sp" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:orientation="vertical"
            android:padding="0dp">


            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal">

                <ImageView
                    android:layout_width="200sp"
                    android:layout_height="100sp"
                    android:layout_marginTop="-50sp"
                    android:layout_centerInParent="true"
                    app:srcCompat="@drawable/placa"/>

                <EditText
                    android:id="@+id/clanname2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/samurai"
                    android:text="@string/clan_name"
                    android:textAlignment="center"
                    android:layout_centerInParent="true"
                    android:textColor="@color/black"
                    android:textSize="12sp" />


            </RelativeLayout>


            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal">

                <ImageView
                    android:layout_width="200sp"
                    android:layout_height="100sp"
                    android:layout_marginTop="-50sp"
                    android:layout_centerInParent="true"
                    app:srcCompat="@drawable/placa"/>

                <EditText
                    android:id="@+id/samurainame2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:backgroundTint="@color/transparent"
                    android:fontFamily="@font/samurai"
                    android:text="@string/samurai_name"
                    android:textAlignment="center"
                    android:layout_centerInParent="true"
                    android:textColor="@color/black"
                    android:textSize="12sp" />

            </RelativeLayout>

          <Space
              android:layout_width="match_parent"
              android:layout_height="10sp"/>

            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="180sp"
                android:layout_gravity="center_horizontal"
                android:paddingBottom="10sp">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:gravity="center">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal">

                    <ImageView
                        android:layout_width="60sp"
                        android:layout_height="60sp"
                        android:layout_centerInParent="true"
                        android:clickable="true"
                        android:focusable="true"
                        app:srcCompat="@drawable/placa2"/>

                    <ImageView
                        android:layout_width="30sp"
                        android:layout_height="30sp"
                        android:layout_centerInParent="true"
                        app:srcCompat="@drawable/bamboo"/>

                </RelativeLayout>

                    <RelativeLayout
